enum Week{
    MONDAY,
    TUESDAY,
    WEDAY,
    FRIDAY
}

public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		Week myval = Week.MONDAY;
		System.out.print(myval);
	}
}

enum Week{
    MONDAY,
    TUESDAY,
    WEDAY,
    FRIDAY;
    
    Week()
    {
    System.out.println("hr");
    }
}

