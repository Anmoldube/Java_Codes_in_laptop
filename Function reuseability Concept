import java.util.Scanner;

public class demo
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        String naam1 = sc.nextLine();
        String naam2 = sc.nextLine();
        String naam3 = sc.nextLine();
        String naam4 = sc.nextLine();
        naam(naam1);
        naam(naam2);
        naam(naam3);
        naam(naam4);
        sc.close();
    }
        public static void naam(String naam1)
        {
            for (int i = 0; i <= 10; i++)
            {
                System.out.println(naam1);
            }
        }
}
