public class Polymorphism {
     int sum(int a, int b){
        return a+b;
     }
     int sum(int a, int b, int c){
        return a+b+c;
     }
        public static void main(String[] args) {
            Polymorphism p = new Polymorphism();
            System.out.println(p.sum(2, 3));
            System.out.println(p.sum(2, 3, 4));
        }
}
